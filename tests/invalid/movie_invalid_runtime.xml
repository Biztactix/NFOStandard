<?xml version="1.0" encoding="UTF-8"?>
<!-- Invalid: Runtime must be an integer (minutes), not a string -->
<!-- Expected error: Element 'runtime': 'two hours' is not a valid value of the atomic type 'xs:integer'. -->
<root xmlns="NFOStandard" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="NFOStandard https://raw.githubusercontent.com/Biztactix/NFOStandard/main/main.xsd">
    <media>
        <movie>
            <title>Invalid Runtime Example</title>
            <runtime>two hours</runtime>
        </movie>
    </media>
</root>